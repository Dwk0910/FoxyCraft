{
    "name": "foxycraft",
    "author": "neatore",
    "version": "0.0.1",
    "type": "module",
    "description": "FoxyCraft helps you create, configure, and run your own Minecraft server with a powerful and user-friendly interface.",
    "main": "main.mjs",
    "scripts": {
        "lint": "eslint .",
        "start-react": "vite",
        "build-react": "vite build",
        "start-electron": "electron .",
        "build": "npm run build-react && electron-builder --dir",
        "dev": "concurrently --kill-others --raw \"npm run lint && npm run start-react\" \"wait-on http-get://localhost:5173/ && npm run start-electron\""
    },
    "dependencies": {
        "@ant-design/v5-patch-for-react-19": "^1.0.3",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.33.0",
        "@tailwindcss/vite": "^4.1.11",
        "antd": "^5.26.7",
        "axios": "^1.11.0",
        "clsx": "^2.1.1",
        "electron-log": "^5.4.2",
        "framer-motion": "^12.23.12",
        "globals": "^16.3.0",
        "i18next": "^25.4.2",
        "jotai": "^2.13.0",
        "jquery": "^3.7.1",
        "lucide-react": "^0.541.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-i18next": "^15.6.1",
        "react-icons": "^5.5.0",
        "react-router-dom": "^7.7.1",
        "react-toastify": "^11.0.5",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.11",
        "@types/react": "^19.1.11",
        "@types/react-dom": "^19.1.7",
        "@typescript-eslint/eslint-plugin": "^8.40.0",
        "@typescript-eslint/parser": "^8.40.0",
        "@vitejs/plugin-react": "^4.7.0",
        "concurrently": "^9.2.0",
        "electron": "^37.2.5",
        "electron-builder": "^26.0.12",
        "eslint": "^9.34.0",
        "eslint_d": "^14.3.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "prettier": "^3.6.2",
        "tailwindcss": "^4.1.12",
        "vite": "^7.1.3",
        "wait-on": "^8.0.4"
    },
    "build": {
        "appId": "org.foxycraft",
        "productName": "FoxyCraft",
        "files": [
            "dist/**/*",
            "main.js",
            "preload.js"
        ],
        "extraFiles": [
            {
                "from": "resources/",
                "to": "resources/",
                "filter": "**/*"
            }
        ],
        "directories": {
            "output": "release"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dwk0910/FoxyCraft.git"
    },
    "keywords": [],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/Dwk0910/FoxyCraft/issues"
    },
    "homepage": "https://github.com/Dwk0910/FoxyCraft#readme"
}
